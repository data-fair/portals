    "streamsaver": "^2.0.6",
    "vue-social-sharing": "^3.0.4",
    "web-streams-polyfill": "^3.2.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.5.0",
    "@commitlint/config-conventional": "^17.4.4",
    "@data-fair/frame": "^0.18.0",
    "@data-fair/sd-vue": "4.0.0",
    "@koumoul/vjsf": "2.22.1",
    "@koumoul/vue-multianalytics": "^1.11.4",
