{
  "compilerOptions": {
    "target": "esnext",
    "module": "nodenext",
    "moduleResolution": "NodeNext",
    "allowImportingTsExtensions": true,
    "noEmit": true,
    "strict": true,
    "allowJs": true,
    "checkJs": true,
    "skipLibCheck": true
  },
  "exclude": ["node_modules", "types", "ui", "portal", "dev", "api/assets", "google-fonts-complete/"],
  "types": ["vuetify"]
}